number = int(input('Enter number of integers you want to enter in the list \n'))
lst = []
for i in range(number):
    n = int(input())
    lst.append(n)
def swap(lis):
    popped = lis.pop()
    X = lis[0]
    lis.remove(X)
    lis.insert(0,popped)
    lis.append(X)
    return lis

swap(lst)
print(lst)
